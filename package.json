{
  "name": "@canva/closure-compiler-externs-generator",
  "version": "1.0.3",
  "description": "Generates externs for closure compiler from TypeScript declarations",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "repository": "git@github.com:canva-public/closure-compiler-externs-generator.git",
  "author": "Canva Pty Ltd.",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/react": "17.0.34",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.5",
    "husky": ">=6",
    "jest": "^26.6.3",
    "lint-staged": ">=10",
    "memfs": "^3.2.2",
    "prettier": "2.2.1",
    "react": "17.0.2",
    "ts-jest": "^26.5.4"
  },
  "scripts": {
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --write .",
    "pretest": "prettier --check . && yarn eslint",
    "test": "jest --ci --coverage",
    "test:watch": "jest --watch --notify",
    "preprepare": "husky install",
    "prepack": "tsc --declarationDir dist --declaration",
    "prepare": "rm -rf ./dist"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,css,md}": "prettier --write"
  },
  "dependencies": {
    "fast-glob": "^3.2.11",
    "typescript": "^4.2.4",
    "yargs": "^17.3.1"
  }
}
