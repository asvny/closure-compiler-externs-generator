{
  "name": "@canva/closure-compiler-externs-generator",
  "version": "1.0.0",
  "description": "Generates externs for closure compiler from TypeScript declarations",
  "main": "index.js",
  "repository": "git@github.com:canva-public/closure-compiler-externs-generator.git",
  "author": "Canva Pty Ltd.",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@sindresorhus/slugify": "^1.1.0",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.5",
    "husky": ">=6",
    "jest": "^26.6.3",
    "lint-staged": ">=10",
    "memfs": "^3.2.2",
    "prettier": "2.2.1",
    "react": "^17.0.2",
    "ts-jest": "^26.5.4"
  },
  "scripts": {
