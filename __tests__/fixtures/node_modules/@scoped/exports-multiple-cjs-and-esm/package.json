{
    "name": "exports-multiple-cjs-and-esm",
    "main": "src/main.js",
    "module": "src/main.mjs",
    "types": "src/main.d.ts",
    "exports": {
        ".": {
            "import": "./src/main.mjs",
            "require": "./src/main.js"
        },
        "./extra": {
            "require": "./extra/src/extra.js",
            "import": "./extra/src/extra.mjs"
        }
    }
}